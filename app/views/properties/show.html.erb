              html.mapEnabled #mapContainer {
              position: fixed;
              left: 0;
              top: 0;
              width: 100%;
              height: 100%; /* this line covers the viewport */
              z-index: 999999999;
              }
              html.mapEnabled #mapBox {
              position: absolute;
              top: 0;
              left: 0;
              right: 0;
              bottom: 60px;
              height: auto;
              }
              html.mapEnabled #mapFooter {
              display: block;
              position: absolute;
              bottom: 0;
              }
              }
