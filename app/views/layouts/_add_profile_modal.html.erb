<div class="addprofile_block">
  <button type="button" id="add_acc_btn" class="js-open-modal" style="display: none;" data-target="#myModal">dxf</button>
  <div class="modal fade profile_modal" id="myModal" tabindex="-1" role="dialog" aria-labelledby="myModalLabel">
    <div class="modal-dialog wide-dialog" role="document">
      <div class="modal-content" id="setpopup_width" style="border-bottom: none;">
        <div class="modal-header white-header">
          <button type="button" class="close" data-dismiss="modal" aria-label="Close" ><span aria-hidden="true">&times;</span></button>
          <h4 class="modal-title acc-title" id="myModalLabel">CONNECT AN ACCOUNT </h4>
        </div>
        <div class="modal-body connect-modal-body">
          <div class="row">
            <div class="col-md-3 col-sm-3 col-xs-6 col-centered text-center nopadding">
              <div class="col-sm-12 net-border">
                <div class="row">
                  <div class="col-sm-12 acc-net-title acc-facebook">FACEBOOK</div>
                </div>
                <div class="row">
                  <div class="col-sm-12 net-logos">
                    <i class="fa fa-facebook" style="color:#3c5a98;"></i>
                  </div>
                </div>
                <div class="row">
                  <div class="col-sm-12 acc-btns">
                    <a data-turbolinks="false" href="<%= Rails.env.production? ? 'http://app.houseq.io' : 'http://dev.house.com' %>/fb/fbconfig.php?current_user_id=<%= current_user.id %>" class="btn btn-default publish_tab_btn" style="background-color:#3c5a98; font-size:10px; ">CONNECT PROFILE</a>
                    <a data-turbolinks="false" href="<%= Rails.env.production? ? 'http://app.houseq.io' : 'http://dev.house.com' %>/fb/fbconfig.php?con_type=page&current_user_id=<%= current_user.id %>" class="btn btn-default publish_tab_btn" style="background-color:#3c5a98; font-size:10px; ">CONNECT PAGE</a>
                  </div>
                </div>
              </div>
            </div>
            <div class="col-md-3 col-sm-3 col-xs-6 col-centered text-center nopadding">
              <div class="col-sm-12 net-border">
                <div class="row">
                  <div class="col-sm-12 acc-net-title acc-twitter">TWITTER</div>
                </div>
                <div class="row">
                  <div class="col-sm-12 net-logos">
                    <i class="fa fa-twitter" style="color:#54abee;"></i>
                  </div>
                </div>
                <div class="row">
                  <div class="col-sm-12 acc-btns">
                   <a data-turbolinks="false" href="<%= Rails.env.production? ? 'http://app.houseq.io' : 'http://dev.house.com' %>/twitter/index.php?current_user_id=<%= current_user.id %>" class="btn btn-default publish_tab_btn" style="background-color:#54abee; font-size:10px;">CONNECT PROFILE</a>
                  </div>
                </div>
              </div>
            </div>
            <div class="col-md-3 col-sm-3 col-xs-6 col-centered text-center nopadding">
              <div class="col-sm-12 net-border">
                <div class="row">
                  <div class="col-sm-12 acc-net-title acc-linkedin">LINKEDIN</div>
                </div>
                <div class="row">
                  <div class="col-sm-12 net-logos">
                    <i class="fa fa-linkedin" style="color:#007bb6;"></i>
                  </div>
                </div>
                <div class="row">
                  <div class="col-sm-12 acc-btns">
                    <a data-turbolinks="false" href="<%= Rails.env.production? ? 'http://app.houseq.io' : 'http://dev.house.com' %>/linkedin_logout_script.html?current_user_id=<%= current_user.id %>" class="btn btn-default publish_tab_btn" style="background-color:#007bb6; font-size:10px; ">CONNECT PROFILE</a>
                    <a data-turbolinks="false" href="<%= Rails.env.production? ? 'http://app.houseq.io' : 'http://dev.house.com' %>/linkedin_logout_script.html?con_type=page&current_user_id=<%= current_user.id %>" class="btn btn-default publish_tab_btn" style="background-color:#007bb6; font-size:10px;">CONNECT PAGE</a>
                  </div>
                </div>
              </div>
            </div>
            <div class="col-md-3 col-sm-3 col-xs-6 col-centered text-center nopadding">
              <div class="col-sm-12 net-border">
                <div class="row">
                  <div class="col-sm-12 acc-net-title acc-instagram">INSTAGRAM</div>
                </div>
                <div class="row">
                  <div class="col-sm-12 net-logos">
                    <i class="fa fa-instagram" style="color:#4D423C;"></i>
                  </div>
                </div>
                <div class="row">
                  <div class="col-sm-12 acc-btns">
                    <a data-turbolinks="false" href="<%= Rails.env.production? ? 'https://app.houseq.io' : 'http://192.168.0.37:3003' %>/add_for_schedule" class="btn btn-default publish_tab_btn" style="background-color:#4D423C; font-size:10px;">CONNECT PROFILE </a>
                  </div>
                </div>
              </div>
            </div>
          </div>
        </div>
        <div class="modal-footer white-footer">
          <button type="button" class="btn btn-link acc-btn-red" data-dismiss="modal">DONE</button>
        </div>
      </div>
    </div>
  </div>
</div>